    "@shelf/jest-dynamodb": "^1.4.1",
    "@types/aws-lambda": "^8.10.61",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.10",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^10.14.21",
    "aws-sdk": "^2.585.0",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2",
    "merge": "^1.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.2"
  }
}
